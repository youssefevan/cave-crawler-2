config_version=5

[application]

config/name="CaveCrawler2"
run/main_scene="res://scenes/gui/menus/menu_main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.svg"

[autoload]

Global="*res://scripts/global.gd"
AudioHandler="*res://scenes/audio_handler.tscn"
OptionsHandler="*res://scenes/options_handler.tscn"

[display]

window/size/viewport_width=256
window/size/viewport_height=144
window/size/window_width_override=1024
window/size/window_height_override=576
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://scenes/": "blue",
"res://scripts/": "yellow",
"res://sprites/": "red"
}

